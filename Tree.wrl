#VRML V2.0 utf8

Group {
	children [
		Shape {
			appearance Appearance {
				texture	ImageTexture {
					url	"bark.jpg"
					repeatS	TRUE
					repeatT	FALSE
				}
			}
			geometry Cylinder {
				radius 2
				height 7.5
				bottom TRUE
				side TRUE
				top	FALSE
			}
		}
	]
}
Transform {
	translation	0 8.0 0
	children [
		Shape {
			appearance Appearance {
				material Material {
					diffuseColor 0.0 0.3 0
					shininess 1
				}
			} 
			geometry Sphere{
				radius 5
			}
		}
	]
}